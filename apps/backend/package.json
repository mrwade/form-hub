{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "volta": {
    "node": "18.12.0"
  },
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "build:prisma": "prisma generate && yarn rebuild:be",
    "db:console": "docker compose exec postgres psql -h localhost -U postgres -d webapp_dev",
    "db:migrate": "npx knex migrate:latest",
    "docker:db:migrate": "docker compose run backend yarn db:migrate",
    "rebuild:be": "docker compose build backend && docker compose rm --stop backend && docker compose up -d backend",
    "start": "node dist/index.js",
    "start:docker": "yarn build && node --inspect=0.0.0.0 dist/index.js"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@prisma/client": "4.1.1",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "bullmq": "^1.86.10",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.2.3",
    "knex": "^2.2.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^3.0.0",
    "pg": "^8.7.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.127",
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "prisma": "^4.1.1",
    "rimraf": "^3.0.2"
  }
}
